int[] n = new int[10];
        String str = "903327088";
        char[] nums = str.toCharArray();
        for(int i=0;i<str.length();i++){
            int j = Integer.valueOf(nums[i])-48;
            n[j]++;
        }
        System.out.print("对应的数字：");
        for(int k=0;k<10;k++)
           System.out.print(k+" "); 
        System.out.print("\n");
        System.out.print("出现的次数：");
        for(int k=0;k<10;k++)
           System.out.print(n[k]+" "); 

